<% @following.each do |f| %>

	<center>
		<div class="well">
			<a href="<%= locations_view_url(:id => f.id) %>" style="font-size:18px; z-index:99999;" class="tooltip-info" title="" data-original-title="View Profile" data-placement="bottom">
				<%= f.busName %>
			</a>
			<br/>
			<% unless f.locName.blank? %>
				<span class="infobox-content" style="font-size:12px;"><%= f.locName %></span>
			<% end %>
		</div>
	</center>

<% end %>